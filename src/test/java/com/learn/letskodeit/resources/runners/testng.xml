<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="testsuite" verbose="1" >
    <listeners>
        <listener class-name="com.learn.letskodeit.listeners.Listeners"></listener>
        <listener class-name="org.uncommons.reportng.HTMLReporter"></listener>
        <listener class-name="org.uncommons.reportng.JUnitXMLReporter"></listener>
    </listeners>
    <test name="Lets kode it test" >
        <classes>
            <class name="com.learn.letskodeit.testsuite.LoginTest"/>
            <class name="com.learn.letskodeit.testsuite.PracticeTest"/>
            <class name="com.learn.letskodeit.testsuite.SignUpTest"/>
        </classes>
    </test>
</suite>
